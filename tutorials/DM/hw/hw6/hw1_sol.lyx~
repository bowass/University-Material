\begin_deeper
\begin_layout Enumerate
תהי 
\begin_inset Formula $S$
\end_inset

 קבוצת הסטודנטים,
 פרדיקט 
\begin_inset Formula $T\left(x\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $x$
\end_inset

 מצליח במבחן
\begin_inset Quotes erd
\end_inset

 ופרדיקט 
\begin_inset Formula $H\left(x\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $x$
\end_inset

 עושה את תרגילי הבית
\begin_inset Quotes erd
\end_inset

.
 הטענה היא
\begin_inset Formula 
\[
\forall x\in S:\left(T\left(x\right)\rightarrow H\left(x\right)\right),
\]

\end_inset

ושלילת הטענה לאחר פישוט היא
\begin_inset Formula 
\[
\exists x\in S:\left(T\left(x\right)\wedge\neg H\left(x\right)\right).
\]

\end_inset


\end_layout

\begin_layout Enumerate
תהי 
\begin_inset Formula $H$
\end_inset

 קבוצת בני האדם,
 פרידקט 
\begin_inset Formula $D\left(x\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $x$
\end_inset

 אוהב מתמטיקה דיסקרטית
\begin_inset Quotes erd
\end_inset

 ופרידקט 
\begin_inset Formula $I\left(x\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $x$
\end_inset

 אוכל גלידה
\begin_inset Quotes erd
\end_inset

.
 הטענה היא
\begin_inset Formula 
\[
\forall x\in H:\left(D\left(x\right)\rightarrow I\left(x\right)\right),
\]

\end_inset

ושלילת הטענה לאחר פישוט היא
\begin_inset Formula 
\[
\exists x\in H:\left(D\left(x\right)\wedge\neg I\left(x\right)\right).
\]

\end_inset


\end_layout

\begin_layout Enumerate
תהי 
\begin_inset Formula $P$
\end_inset

 קבוצת הראשוניים,
 ונגדיר פרדיקט 
\begin_inset Formula $D\left(x,y\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $x$
\end_inset

 מחלק את 
\begin_inset Formula $y$
\end_inset


\begin_inset Quotes erd
\end_inset

 (כלומר 
\begin_inset Formula $x\mid y$
\end_inset

).
 הטענה היא
\begin_inset Formula 
\[
\forall n\in\mathbb{N}\forall p\in P:\neg D\left(p,n\right),
\]

\end_inset

ושלילת הטענה היא
\begin_inset Formula 
\[
\exists n\in\mathbb{N}\exists p\in P:D\left(p,n\right).
\]

\end_inset


\end_layout

\begin_layout Enumerate
תהי 
\begin_inset Formula $B$
\end_inset

 קבוצת הבניינים,
 פרדיקט 
\begin_inset Formula $T\left(b\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $b$
\end_inset

 בן יותר מ-
\begin_inset Formula $100$
\end_inset

 קומות
\begin_inset Quotes erd
\end_inset

 ופרדיקט 
\begin_inset Formula $U\left(b\right)$
\end_inset

 ל-
\begin_inset Quotes erd
\end_inset


\begin_inset Formula $b$
\end_inset

 נמצא באוניברסיטה
\begin_inset Quotes erd
\end_inset

.
 הטענה היא
\begin_inset Formula 
\[
\exists b\in B:\left(T\left(b\right)\wedge\neg U\left(b\right)\right),
\]

\end_inset

ושלילת הטענה היא
\begin_inset Formula 
\[
\forall b\in B:\left(\neg T\left(b\right)\vee U\left(b\right)\right),
\]

\end_inset

או לחילופין 
\begin_inset Formula $\forall b\in B:T\left(b\right)\rightarrow U\left(b\right)$
\end_inset

.
\end_layout

\begin_layout Enumerate
הטענה היא
\begin_inset Formula 
\[
\exists\alpha,\beta\in\mathbb{R}:\left(\alpha>\beta\wedge\alpha^{2}<\beta^{2}\wedge\alpha^{3}>\beta^{3}\right),
\]

\end_inset

ושלילת הטענה היא
\begin_inset Formula 
\[
\forall\alpha,\beta\in\mathbb{R}:\left(\alpha\le\beta\vee\alpha^{2}\ge\beta^{2}\vee\alpha^{3}\le\beta^{3}\right).
\]

\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

